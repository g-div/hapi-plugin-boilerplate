{
  "name": "hapi-plugin-boilerplate",
  "author": "g-div",
  "version": "0.0.1",
  "description": "hapi plugin",
  "devDependencies": {
    "babel": "^5.2.6",
    "code": "^1.4.0",
    "hapi": "^11.1.1",
    "husky": "^0.10.2",
    "lab": "^5.8.0",
    "node-readme": "^0.1.9",
    "npm-run-all": "^1.3.1"
  },
  "peerDependencies": {
    "hapi": ">= 8.x.x"
  },
  "main": "dist/index.js",
  "scripts": {
    "pretest": "npm run compile",
    "test": "lab --transform test/transformer.js -t 100",
    "precoverage": "npm run compile",
    "coverage": "lab --transform test/transformer.js -r html -o coverage.html",
    "compile": "babel -s -d dist/ src/",
    "postcompile": "npm run readme",
    "readme": "node-readme",
    "prepush": "npm-run-all compile test",
    "prepublish": "npm-run-all compile test",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "postpublish": "git push origin master --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:g-div/hapi-plugin-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/g-div/hapi-plugin-boilerplate/issues"
  },
  "keywords": [
    "hapi"
  ],
  "license": "MIT"
}
